<div>
    <h4>添加新联系人信息</h4>

    <form name="addressForm" >
        <div class="form-group">
                <label for="recipient">收件人</label>
                <input type="text" id="recipient" class="form-control" ng-model="address.recipient"/></div>
        <div class="form-group"><label for="province">国家</label>
                <input type="text" id="country" class="form-control" ng-model="address.country"/></div>
        <div class="form-group"><label for="province">省</label>
                <input type="text" id="state" class="form-control" ng-model="address.state"/></div>
        <div class="form-group"><label for="city">市</label>
                <input type="text" id="city" class="form-control" ng-model="address.city"/></div>
        <div class="form-group"><label for="district">区</label>
                <input type="text" id="district" class="form-control" ng-model="address.suburb"/></div>
        <div class="form-group"><label for="street">街道</label>
                <input type="text" id="street" class="form-control" ng-model="address.street1"/></div>
        <div class="form-group"><label for="business">公司</label>
                <input type="text" id="business" class="form-control" ng-model="address.street2"/></div>
        <div class="form-group"><label for="recipient">邮编</label>
        <input type="text" id="postalCode" class="form-control" ng-model="address.postalCode"/></div>
<div class="form-group"><label for="mobile">手机号</label>
        <input type="text" id="contactNumber" class="form-control" ng-model="address.contactNumber"/></div>

            <button type="submit" ng-click="saveAddressSubmit()" >保存</button>
    </form>
</div>

