<div class="container-fluid">
    <form class="form-horizontal">

        <div class="form-group">
            <div class="col-lg-6">
                <label for="name">姓名</label>
                <input class="form-control" ng-model="freight.user.realName">
                <label for="address">地址</label>
                <input class="form-control" ng-model="freight.address">
                <label for="mobile">手机号</label>
                <input class="form-control" ng-model="freight.user.mobiePhoneNumber">
            </div>
            <div class="col-lg-6">
                <label for="trackingNumber">入库号</label>
                <input class="form-control" ng-model="freight.trackingNumber">
                <label for="createdAt">入库时间</label>
                <input class="form-control" ng-model="freight.createdAt">
                <label for="weight">重量</label>
                <input class="form-control" ng-model="freight.weight">
            </div>
        </div>
        <div class="form-group">
            <label class="col-lg-2">等待用户最后确认</label>
            <input class="col-lg-2" ng-model="freight.a">

            <label class="col-lg-2">等待加固</label>
            <input class="col-lg-2" ng-model="freight.b">

            <label class="col-lg-2">等待减重</label>
            <input class="col-lg-2" ng-model="freight.c">
        </div>
        <div class="form-group">
            <label class="col-lg-2">等待开箱检查</label>
            <input class="col-lg-2" ng-model="freight.d">

            <label class="col-lg-2">等待普通分箱</label>
            <input class="col-lg-2" ng-model="freight.e">

            <label class="col-lg-2">等待精确分箱</label>
            <input class="col-lg-2" ng-model="freight.f">
        </div>
    </form>
</div>